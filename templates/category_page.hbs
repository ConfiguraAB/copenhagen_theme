<!-- CUSTOM SIDEBAR -->
<main class="main-sidebar" role="main">
<div id="sidebar" class="sidebar-panel">
  <ul class="sidebar">
    <li class="sidebar-item sidebar-home open">
      <a href="https://www.configura.com/academy/cet-designer" class="custom-sidebar-item-title" target="_blank">
        <i class="material-icons custom-sidebar-icon custom-icon-margin-right blue-icon">school</i>
         Academy and training
      </a>
    </li>
    <li class="sidebar-item sidebar-home open">
      <a href="https://www.configura.com/products/system-recommendations/cet-designer" class="custom-sidebar-item-title" target="_blank">
        <i class="material-icons custom-sidebar-icon custom-icon-margin-right blue-icon">computer</i>
         System recommendations
      </a>
    </li>
    <li class="sidebar-item sidebar-home open">
      <a href="https://www.configura.com/products/release-notes/cet-designer" class="custom-sidebar-item-title" target="_blank">
        <i class="material-icons custom-sidebar-icon custom-icon-margin-right blue-icon">import_contacts</i>
         Release notes
      </a>
    </li>
    <li class="sidebar-item sidebar-home open custom-margin-bottom">
      <a href=" https://www.configura.com/my/forum" class="custom-sidebar-item-title" target="_blank">
        <i class="material-icons custom-sidebar-icon custom-icon-margin-right blue-icon">forum</i>
         Forums
      </a>
    </li>
    <li :class="['sidebar-item', isOpen(category.id)]" v-if="category.id !== 360002370413" v-for='category in categories' :data-id='category.id'>
      <h4 class="sidebar-item-title" @click='setActiveSection(category.id)' v-cloak>{[{category.name}]}</h4>   
      <ul v-if='category.id === activeSection'>
        <li :class="['sidebar-item', isCurrent(section.id)]" v-for='section in category.sections'>
          <a :href='section.html_url' class="sidebar-item-link">
            {[{section.name}]} 
          </a>
        </li>
      </ul>
    </li>
  </ul>
</div>
  
<!-- END CUSTOM SIDEBAR -->

<div class="container-divider"></div>
<div class="custom-container container">
    <nav class="sub-nav">
      {{breadcrumbs}}
      {{search scoped=settings.scoped_kb_search submit=false placeholder='Search for help articles, videos and more'}}
    </nav>
  <div class="category-container custom-category-container">
    <div class="category-content">
      <header class="page-header">
        <h1>{{category.name}}</h1>
        {{#if category.description}}
          <p class="page-header-description">{{category.description}}</p>
        {{/if}}
      </header>
      <!-- CUSTOM CARDS -->
      <div class="custom-card-container">
        {{#each sections}}
        	<section class="custom-card small-card">
            	<a href="{{url}}" class="custom-card-link">
              			<div class="custom-card-logo-container">
						{{#is id 360006621153}} {{!-- Video Tutorials --}}
				   			<i class="material-icons black-icon">ondemand_video</i>
				  		{{else}}
				  			{{#is id 360005899854}} {{!-- Drawing in CET Designer --}}
					  			<i class="material-icons black-icon">brush</i>
							{{else}}
								{{#is id 360005926133}} {{!-- Component Tabs --}}
									<i class="material-icons black-icon">tab</i>
								{{else}}
									{{#is id 360005899834}} {{!-- Additional Extension --}}
										<i class="material-icons black-icon">layers</i>
									{{else}}
										{{#is id 360005931333}} {{!-- Calculations --}}
											<i class="material-icons black-icon">dialpad</i>
									{{else}}
										{{#is id 360003354354}} {{!-- FAQ --}}
											<i class="material-icons black-icon">help</i>
										{{else}}
