																<i class="material-icons black-icon">assignment</i>
															{{else}}
																{{#is id 360006369353}} {{!-- Materials --}}
																	<i class="material-icons black-icon">style</i>
																{{else}}
																	{{#is id 360006341494}} {{!-- Features --}}
																		<i class="material-icons black-icon">stars</i>
																	{{else}}
																		{{#is id 360006369293}} {{!-- Keyboard Shortcuts --}}
																			<i class="material-icons black-icon">keyboard</i>
																		{{else}}
