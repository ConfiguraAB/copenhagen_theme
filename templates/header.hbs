<!-- Notification banner -->
<div class="alertbox"></div>
<!-- Custom header -->
<div class="custom-background-color-blue">
  <header class="header container">
    <div class="custom-header-container">
      {{#link 'help_center' class='custom-logo logo'}}
        <img src="{{settings.logo}}" alt="{{t 'home_page' name=help_center.name}}">
      {{/link}}
      {{dc 'to_home_page'}}

    </div>
    <div class="custom-nav-wrapper nav-wrapper">
      <span class="icon-menu" tabindex="0" role="button" aria-controls="user-nav" aria-expanded="false"
        aria-label="{{t 'toggle_navigation'}}">
      </span>
      {{dc 'header_links'}}
      <nav class="user-nav custom-display-links" id="user-nav">
        <a href="/"></a>
        <!-- To add links to burger menu look at script.js CUSTOM MOBILE MENU ITEMS -->
      </nav>
      <div class="custom-signin-link" title="User information">
        {{user_info}}
      </div>
      <div id="language-dd" class="language-display"></div>
    </div>
  </header>
</div>
